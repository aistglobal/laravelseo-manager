{
    "name": "aistglobal/seomanager",
    "description": "Seo Manager For Laravel",
    "keywords": [
        "Seo",
        "Manager",
        "laravel5",
        "laravel5.6",
        "seo",
        "SeoManager",
        "webmaster",
        "meta tags",
        "opengraph",
        "twitter",
        "manager"
    ],
    "homepage": "https://github.com/AIST-Global/laravel-seo-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "AIST Global",
            "email": "info@aist.global",
            "homepage": "https://github.com/aistglobal",
            "role": "Developer"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "~5.1",
        "php": "~5.6|~7.0",
        "laravelcollective/html": "^5.4",
        "artesaos/seotools": "^0.10.0",
        "league/flysystem-aws-s3-v3": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Seo\\Manager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seo\\Manager\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR4 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR4 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"


    },
    "config": {
        "preferred-install": "dist"
    }
}
